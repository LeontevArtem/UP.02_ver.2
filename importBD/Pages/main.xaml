<Page x:Class="importBD.Pages.main"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:uc ="clr-namespace:WpfControlLibrary2.Elements;assembly=WpfControlLibrary2"
      xmlns:local="clr-namespace:importBD.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="main">

    <Grid Background="#E9F1FA">
        <Grid x:Name="TopGrid" Height="50" VerticalAlignment="Top">
            <Label Content="Импорт данных в базу данных" FontSize="20" FontWeight="Bold" VerticalAlignment="Center"/>
        </Grid>
        <Grid Margin="0,50,0,0">
            <uc:Button1 VerticalAlignment="Bottom" XAMLTextColor="White" XAMLText="Импортировать" Margin="184,0,10,5" Height="30" MouseDown="importClick"/>
            <uc:Button1 x:Name="BackButton" XAMLTextColor="White" XAMLText="Назад" VerticalAlignment="Bottom" Margin="10,0,0,5" Height="30" HorizontalAlignment="Left" Width="174"/>
        </Grid>
    </Grid>
</Page>
