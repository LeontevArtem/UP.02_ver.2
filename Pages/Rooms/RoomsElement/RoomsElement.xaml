<UserControl x:Class="UP._02_ver._2.Pages.Rooms.RoomsElement.RoomsElement"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UP._02_ver._2.Pages.Rooms.RoomsElement"
      mc:Ignorable="d" 
      >

    <Border Background="#FFBFBFBF" CornerRadius="8" Margin="10">
        <Grid x:Name="Parrent" Margin="10">
            <Label x:Name="Name" Content="Название аудитории" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="14" FontWeight="Bold"/>
            <Label x:Name="Short_name" Content="Краткое имя" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0,49,0,0" FontSize="14" FontWeight="Bold"/>
            <Label x:Name="Temp_user" Content="Временный пользователь" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="282,58,0,0" FontSize="14" FontWeight="Bold"/>
            <Label x:Name="User" Content="Ответственный пользователь" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="282,0,0,0" FontSize="14" FontWeight="Bold"/>
        </Grid>
    </Border>
</UserControl>
