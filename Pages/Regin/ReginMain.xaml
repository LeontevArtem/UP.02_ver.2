<Page x:Class="UP._02_ver._2.Pages.Regin.ReginMain"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UP._02_ver._2.Pages.Regin"
      xmlns:uc ="clr-namespace:WpfControlLibrary2.Elements;assembly=WpfControlLibrary2"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ReginMain">

    <Grid Background="#E9F1FA">

        <Border  Background="White" VerticalAlignment="Center" HorizontalAlignment="Center" Width="300" Height="400" CornerRadius="10" >
            <Border.Effect>
                <DropShadowEffect BlurRadius="10" Direction="315" Color="#E9F1FA" ShadowDepth="10"/>
            </Border.Effect>
            <Grid x:Name="RegGrid">
                <Label Content="Регистрация" FontSize="20" FontWeight="Bold" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="10"/>
                <Frame x:Name="RegFrame" Margin="0,55,0,0" NavigationUIVisibility="Hidden"/>
            </Grid>
        </Border>

    </Grid>
</Page>
